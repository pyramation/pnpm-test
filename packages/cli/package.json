{
  "name": "@webql/cli",
  "version": "0.0.0",
  "description": "Command line interface for WebQL",
  "main": "./index.js",
  "types": "./index.d.ts",
  "module": "./index.mjs",
  "bin": {
    "webql": "./cli.js"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "vitest"
  },
  "keywords": [
    "webql",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "@webql/core": "workspace:*"
  }
}